<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
        tools:context=".BlueButtons">


    <Button
        android:id="@+id/MIprev"
        android:layout_width="200dp"
        android:layout_height="55dp"
        android:layout_marginTop="8dp"
        android:backgroundTint="#90E0EF"
        android:text="Prevention"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/MIrf" />

    <Button
        android:id="@+id/MIov"
        android:layout_width="200dp"
        android:layout_height="55dp"
        android:layout_marginTop="15dp"
        android:backgroundTint="#90E0EF"
        android:text="Overview of mental illness"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/MIsym"
        android:layout_width="200dp"
        android:layout_height="55dp"
        android:layout_marginTop="15dp"
        android:backgroundTint="#90E0EF"
        android:text="Symtoms"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/MIov" />

    <Button
        android:id="@+id/MIdoc"
        android:layout_width="200dp"
        android:layout_height="55dp"
        android:layout_marginTop="15dp"
        android:backgroundTint="#90E0EF"
        android:text="When to see a doctor"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/MIcuz" />

    <Button
        android:id="@+id/MIhelpL"
        android:layout_width="200dp"
        android:layout_height="55dp"
        android:layout_marginTop="16dp"
        android:backgroundTint="#90E0EF"
        android:text="Helping a loved one"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/MIdoc" />

    <Button
        android:id="@+id/MIrf"
        android:layout_width="200dp"
        android:layout_height="55dp"
        android:layout_marginTop="12dp"
        android:backgroundTint="#90E0EF"
        android:text="Risk factors"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/MIhelpL" />

    <Button
        android:id="@+id/MImms"
        android:layout_width="286dp"
        android:layout_height="64dp"
        android:layout_marginTop="15dp"
        android:backgroundTint="#90E0EF"
        android:text="Mild, moderate and severe mental health problems"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/MIcom"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/MIcom"
        android:layout_width="200dp"
        android:layout_height="55dp"
        android:layout_marginTop="15dp"
        android:backgroundTint="#90E0EF"
        android:text="Common mental illnesses"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/MIprev" />

    <Button
        android:id="@+id/MIcuz"
        android:layout_width="200dp"
        android:layout_height="55dp"
        android:layout_marginTop="15dp"
        android:backgroundTint="#90E0EF"
        android:text="Causes"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/MIsym" />
</androidx.constraintlayout.widget.ConstraintLayout>



